	pages = {796--9}
}

@article{gosetti_signal_2010,
	title = {Signal suppression/enhancement in high-performance liquid chromatography tandem mass spectrometry},
	volume = {1217},
	abstract = {The review discusses the pitfalls of the matrix effect in mass spectrometry detection hyphenated to liquid chromatography separation. Matrix effect heavily influences both qualitative and quantitative analyses, giving rise to suppression or enhancement of the signal. As generally recognised, the predominant cause is the presence of undesired components that co-elute in the chromatographic separation and alter the ionisation process. The interfering species can be components of the sample, compounds released during the pre-treatment/extraction process or reagents added to the mobile phase to improve chromatographic resolution. The different mechanisms proposed in literature to explain the suppression or the enhancement of the signal both in electrospray and atmospheric pressure chemical ionisations are presented and the results observed in the different experimental conditions are compared and discussed. All data together lead to conclude that the chemical properties of the target analyte, the kind of matrix, the matrix to analyte concentration ratio, the extraction process, the chromatographic conditions as well as the kind of the mass spectrometry instrumentation and the ionisation conditions can play a role. Likely all these potential causes act in a synergic way and the final effect observed is hardly due to only one of them. Depending on an unpredictable combination of conditions, signal suppression or enhancement can be observed. The review discusses the matrix effects observed in HPLC-MS and HPLC-MS/MS analysis proposes hypotheses to explain the observed behaviours and proposes methods and strategies to overcome the matrix effects.},
	number = {25},
	journal = {J Chromatogr A},
	author = {Gosetti, F. and Mazzucco, E. and Zampieri, D. and Gennaro, M. C.},
	year = {2010},
	keywords = {Calibration Chromatography, High Pressure Liquid/*instrumentation/methods/standards Tandem Mass Spectrometry/*instrumentation/methods/standards},
	pages = {3929--37}
}

@article{graether_freezing_2003,
	title = {Freezing of a fish antifreeze protein results in amyloid fibril formation},
	volume = {84},
	abstract = {Amyloid is associated with a number of diseases including Alzheimer's, Huntington's, Parkinson's, and the spongiform encephalopathies. Amyloid fibrils have been formed in vitro from both disease and nondisease related proteins, but the latter requires extremes of pH, heat, or the presence of a chaotropic agent. We show, using fluorescence spectroscopy, electron microscopy, and solid-state NMR spectroscopy, that the alpha-helical type I antifreeze protein from the winter flounder forms amyloid fibrils at pH 4 and 7 upon freezing and thawing. Our results demonstrate that the freezing of some proteins may accelerate the formation of amyloid fibrils.},
	number = {1},
	journal = {Biophys J},
	author = {Graether, S. P. and Slupsky, C. M. and Sykes, B. D.},
	year = {2003},
	keywords = {Amyloid beta-Peptides/chemical synthesis/*chemistry/metabolism/*ultrastructure Animals Antifreeze Proteins/chemistry/metabolism/ultrastructure Antifreeze Proteins, Biomolecular Protein Conformation Protein Structure, Electron Nuclear Magnetic Resonance, Fluorescence Temperature, Secondary Spectrometry, Type I/*chemistry/metabolism/*ultrastructure Flounder/metabolism *Freezing Hydrogen-Ion Concentration Microscopy},
	pages = {552--7}
}


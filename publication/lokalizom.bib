	author = {Aurrecoechea, C. and Brestelli, J. and Brunk, B. P. and Dommer, J. and Fischer, S. and Gajria, B. and Gao, X. and Gingle, A. and Grant, G. and Harb, O. S. and Heiges, M. and Innamorato, F. and Iodice, J. and Kissinger, J. C. and Kraemer, E. and Li, W. and Miller, J. A. and Nayak, V. and Pennington, C. and Pinney, D. F. and Roos, D. S. and Ross, C. and Stoeckert, C. J., Jr. and Treatman, C. and Wang, H.},
	year = {2009},
	keywords = {Animals *Databases, Genetic, Genetic *Genome, Protozoan Genomics Plasmodium/*genetics/growth \& development/metabolism Protozoan Proteins/genetics/physiology Transcription},

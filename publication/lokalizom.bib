  journal = {ChemMedChem},
  year = {2015},
  volume = {10},
  pages = {1344-9},
  number = {8},
  abstract = {In a follow-up study to our reports of N-cinnamoylated chloroquine
	and quinacrine analogues as promising dual-stage antimalarial leads
	with high in vitro potency against both blood-stage Plasmodium falciparum
	and liver-stage Plasmodium berghei, we decided to investigate the
	effect of replacing the cinnamoyl moiety with other acyl groups.

	year = {2010},
	keywords = {Data Collection Databases, Genetic/economics Genetic Variation/*genetics Genome, Human/*genetics Humans Individualized Medicine Motivation Mutation/ethics/*genetics Paris Software Terminology as Topic United Nations},

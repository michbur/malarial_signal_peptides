@article{2001speletasanalysis,
   author = {Speletas, M. and Kanariou, M. and Kanakoudi-Tsakalidou, F. and Papadopoulou-Alataki, E. and Arvanitidis, K. and Pardali, E. and Constantopoulos, A. and Kartalis, G. and Vihinen, M. and Sideras, P. and Ritis, K.},

   author = {Vihinen, M. and Belohradsky, B. H. and Haire, R. N. and Holinski-Feder, E. and Kwan, S. P. and Lappalainen, I. and Lehvaslaiho, H. and Lester, T. and Meindl, A. and Ochs, H. D. and Ollila, J. and Vorechovsky, I. and Weiss, M. and Smith, C. I.},
